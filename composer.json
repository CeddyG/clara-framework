    "keywords": ["laravel", "clara", "framework"],
    "authors": [
        {
            "name": "CeddyG"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~6.0",
        "ceddyg/clara-sentinel": "^4.0",
        "ceddyg/clara-parameter": "^4.0",
        "ceddyg/clara-language": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CeddyG\\Clara\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CeddyG\\Clara\\ClaraServiceProvider"
            ]
        }
    }
}
